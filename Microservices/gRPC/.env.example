# Database
# Must match the DATABASE_URL in frontend/.env
DATABASE_URL="postgresql://user:password@localhost:5432/eventflow?schema=public"

# JWT Secret (must match frontend and WebSocket service)
JWT_SECRET="your-jwt-secret-key-here"

# gRPC Server Port
GRPC_PORT=50051

# HTTP Gateway Port (for frontend API calls)
HTTP_PORT=4001

# Node Environment
NODE_ENV="development"
